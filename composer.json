{
    "name": "avto-dev/data-migrations-laravel",
    "description": "Package for data migrations in laravel applications",
    "keywords": [
        "laravel",
        "data",
        "migration"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "avto-dev",
            "homepage": "https://github.com/avto-dev"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": ">=5.5.0 <5.9.0",
        "illuminate/config": ">=5.5.0 <5.9.0",
        "illuminate/database": ">=5.5.0 <5.9.0",
        "illuminate/filesystem": ">=5.5.0 <5.9.0",
        "illuminate/console": ">=5.5.0 <5.9.0",
        "illuminate/contracts": ">=5.5.0 <5.9.0"
    },
    "require-dev": {
        "ext-pdo_sqlite": "*",
        "ext-sqlite3": "*",
        "ext-zlib": "*",
        "avto-dev/dev-tools": "^1.7",
        "laravel/laravel": ">=5.4.0 <5.9.0",
        "mockery/mockery": "^1.0",
        "phpstan/phpstan": "^0.11.3",
        "phpunit/phpunit": "~5.7.10 || ^6.4 || ~7.5"
    },
    "autoload": {
        "psr-4": {
            "AvtoDev\\DataMigrationsLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AvtoDev\\DataMigrationsLaravel\\Tests\\": "tests"
        }
    },
    "scripts": {
        "phpunit": "@php ./vendor/bin/phpunit --no-coverage --colors=always",
        "phpunit-feature": "@php ./vendor/bin/phpunit --no-coverage --colors=always",
        "phpunit-cover": "@php ./vendor/bin/phpunit --coverage-html='./coverage/html'",
        "phpstan": "@php ./vendor/bin/phpstan analyze --no-progress --ansi --level=max ./src",
        "test": [
            "@phpstan",
            "@phpunit"
        ],
        "test-cover": [
            "@phpstan",
            "@phpunit-cover"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "AvtoDev\\DataMigrationsLaravel\\DataMigrationsServiceProvider"
            ]
        }
    },
    "suggest": {
        "ext-zlib": "PHP ZLib extension for a working with compressed (gzip-ed) files"
    },
    "support": {
        "issues": "https://github.com/avto-dev/data-migrations-laravel/issues",
        "source": "https://github.com/avto-dev/data-migrations-laravel"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
